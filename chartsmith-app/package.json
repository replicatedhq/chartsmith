{
  "name": "chartsmith-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "clean-dev": "rm -rf .next && rm -rf node_modules/.cache && next dev --turbopack",
    "dev:clean": "rm -rf .next && rm -rf node_modules/.cache && next dev --turbopack",
    "dev:stable": "rm -rf .next && rm -rf node_modules/.cache && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "./scripts/setup-e2e-tests.sh && (npx playwright test || (cd ../hack/chartsmith-dev && docker compose -f docker-compose.e2e.yml down && exit 1)) && cd ../hack/chartsmith-dev && docker compose -f docker-compose.e2e.yml down",
    "test:e2e:headed": "./scripts/setup-e2e-tests.sh && (npx playwright test --headed || (cd ../hack/chartsmith-dev && docker compose -f docker-compose.e2e.yml down && exit 1)) && cd ../hack/chartsmith-dev && docker compose -f docker-compose.e2e.yml down",
    "test:e2e:setup": "./scripts/setup-e2e-tests.sh && npx playwright test",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:parseDiff": "jest parseDiff"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-toast": "^1.2.7",
    "@tailwindcss/typography": "^0.5.16",
    "@types/diff": "^7.0.1",
    "autoprefixer": "^10.4.20",
    "centrifuge": "^5.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "diff": "^7.0.0",
    "diff-match-patch": "^1.0.5",
    "dotenv": "^16.4.7",
    "gunzip-maybe": "^1.4.2",
    "jotai": "^2.12.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.479.0",
    "monaco-editor": "^0.52.2",
    "next": "^15.2.1",
    "parse-duration": "^2.1.3",
    "parse-git-patch": "^2.1.1",
    "patch": "^0.0.1",
    "patch-package": "^8.0.0",
    "pg": "^8.13.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hotkeys-hook": "^4.6.1",
    "react-markdown": "^10.1.0",
    "secure-random-string": "^1.1.4",
    "tailwind-merge": "^3.0.2",
    "tar": "^7.4.3",
    "unified-diff": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.51.1",
    "@testing-library/react": "^16.2.0",
    "@types/diff-match-patch": "^1.0.36",
    "@types/gunzip-maybe": "^1.4.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.9",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/secure-random-string": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.22.0",
    "eslint-config-next": "^15.2.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
